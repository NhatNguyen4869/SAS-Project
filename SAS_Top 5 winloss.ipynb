   "source": [
    "proc sql;\n",
    "create table tb as \n",
    "select \tmemberid,\n",
    "\t\tyear(gamingdate)*100 +Month(gamingdate) as Vint,\n",
    "\t\tcase\t\n",
    "\t\t\twhen find(segment, \"Tier\") > 0 then \"FITO\"\n",
    "\t\t\twhen Segment = \"Casual Junket\" then \"CITO\"\n",
    "\t\t\telse segment\n",
    "\t\tend as Segment,\n",
    "\t\tsum(us_tablewin) as WinLoss,\n",
    "\t\tsum(adj_turnover) as Turnover\n",
    "from SGMK.RT_TABLEDAILY\n",
    "where &sd. <= gamingdate <= &ed.\n",
    "group by 1, 2, 3\n",
    ";\n",
    "proc sql;\n",
    "create table slot as \n",
    "select \tmemberid, 'EG' as Segment,\n",
    "\t\tyear(gamingdate)*100 +Month(gamingdate) as Vint,\n",
    "\t\tsum(us_slotwin) as WinLoss,\n",
    "\t\tsum(coinin) as Turnover\n",
    "from sgmk.rt_slotdaily\n",
    "where &sd. <= gamingdate <= &ed.\n",
    "group by 1, 2, 3\n",
    ";\n",
    "\n",
    "proc sql;\n",
    "create table df as\n",
    "select \n",
    "\tcoalesce(tb.Vint, slot.Vint, b.Vint) as Vint,\n",
    "    coalesce(tb.memberid, slot.memberid, b.memberid) as Memberid,\n",
    "    coalesce(tb.segment, slot.segment, b.segment) as Segment,\n",
    "/*\ttb.WinLoss as a, slot.WinLoss as b,*/\n",
    "    SUM(SUM(tb.WinLoss, slot.WinLoss)) as WinLoss,\n",
    "\tSUM(SUM(tb.Turnover, slot.Turnover)) as Turnover,\n",
    "\tb.Rolling\n",
    "from tb\n",
    "full join slot\n",
    "    on tb.memberid = slot.memberid\n",
    "\tand tb.segment = slot.segment\n",
    "\tand tb.Vint = slot.Vint\n",
    "full join wd_rolling b\n",
    "    on tb.memberid = b.memberid\n",
    "\tand tb.segment = b.segment\n",
    "\tand tb.Vint = b.Vint\n",
    "group by 1, 2, 3\n",
    "having winloss <> .\n",
    "order by winloss asc\n",
    ";\n",
    "quit;\n",
    "\n",
    "/* Step 1: Sort data by Vint, Segment, and descending Winloss */\n",
    "proc sql;\n",
    "    create table df_sorted_desc as\n",
    "    select \n",
    "        a.Vint,\n",
    "        a.memberid,\n",
    "        segment,\n",
    "        currenttier,\n",
    "        catx(\" \", b.firstname, b.lastname) as GuestName,\n",
    "        b.Nationality,\n",
    "        b.Signupdate,\n",
    "        a.winloss,\n",
    "\t\ta.turnover,\n",
    "        a.rolling\n",
    "    from df a\n",
    "    left join sgmk.pp_patronprofile b \n",
    "        on a.memberid = b.memberid\n",
    "    order by a.Vint, a.segment, a.winloss desc;\n",
    "quit;\n",
    "\n",
    "/* Step 2: Keep top 5 winner per Vint & Segment */\n",
    "data df_top5win;\n",
    "    set df_sorted_desc;\n",
    "    by Vint Segment;\n",
    "    retain count;\n",
    "    \n",
    "    if first.Segment then count = 0;\n",
    "    count + 1;\n",
    "\n",
    "    if count <= 5;\n",
    "run;\n",
    "\n",
    "proc sql;\n",
    "    create table df_sorted_asc as\n",
    "    select \n",
    "        a.Vint,\n",
    "        a.memberid,\n",
    "        segment,\n",
    "        currenttier,\n",
    "        catx(\" \", b.firstname, b.lastname) as GuestName,\n",
    "        b.Nationality,\n",
    "        b.Signupdate,\n",
    "        a.winloss,\n",
    "\t\ta.turnover,\n",
    "        a.rolling\n",
    "    from df a\n",
    "    left join sgmk.pp_patronprofile b \n",
    "        on a.memberid = b.memberid\n",
    "    order by a.Vint, a.segment, a.winloss asc;\n",
    "quit;\n",
    "\n",
    "/* Step 2: Keep top 5 loser per Vint & Segment */\n",
    "data df_top5loss;\n",
    "    set df_sorted_asc;\n",
    "    by Vint Segment;\n",
    "    retain count;\n",
    "    \n",
    "    if first.Segment then count = 0;\n",
    "    count + 1;\n",
    "\n",
    "    if count <= 5;\n",
    "run;\n",
    "\n",
    "proc sql;\n",
    "create table top5 as \n",
    "select *\n",
    "from df_top5win\n",
    "union all \n",
    "select * from df_top5loss;\n",
    "quit;"
   ]
  }
